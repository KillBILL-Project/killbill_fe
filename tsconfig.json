{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "noImplicitAny": true,
    "strictNullChecks": true,
    "types": [
      "@types/styled-components-react-native",
      "react-native",
      "jest"
    ],
    "paths": {
      "@apis/*": ["src/apis/*"],
      "@assets/*": ["src/assets/*"],
      "@common/*": ["src/common/*"],
      "@enum/*": ["src/enum/*"],
      "@queries/*": ["src/queries/*"],
      "@hooks/*": ["src/hooks/*"],
      "@styles/*": ["src/styles/*"],
      "@components/*": ["src/components/*"]
    }
  },
}
